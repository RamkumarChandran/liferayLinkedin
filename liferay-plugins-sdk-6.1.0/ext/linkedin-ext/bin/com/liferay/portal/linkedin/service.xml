<?xml version="1.0"?>

<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.1.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_1_0.dtd">

<service-builder package-path="com.liferay.portal.linkedin">

	<namespace>Portal</namespace>

	<entity name="UserLinkedIn" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="id" type="long" primary="true" />

		<column name="companyId" type="long" />

		<column name="userId" type="long" />

		<column name="linkedId" type="String" />

		<!-- Finder methods -->

		<finder name="UserId" return-type="UserLinkedIn" unique="true">
			<finder-column name="userId" />
		</finder>

		<finder name="LinkedId" return-type="UserLinkedIn" unique="true">
			<finder-column name="linkedId" />
		</finder>

		<finder name="L_C_Id" return-type="UserLinkedIn" unique="true">
			<finder-column name="companyId" />
			<finder-column name="linkedId" />
		</finder>

		<finder name="U_L" return-type="UserLinkedIn" unique="true">
			<finder-column name="companyId" />
			<finder-column name="userId" />
			<finder-column name="linkedId" />
		</finder>

	</entity>

</service-builder>